<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.vortex.msp.Mapper.ussMapper">

    <insert id="saveUss" parameterType="com.vortex.msp.Entity.Uss"
            keyProperty="ussId" useGeneratedKeys="true">
        INSERT INTO uss(uss_a, uss_b)
        VALUES (#{ussA}, #{ussB})
    </insert>

</mapper>
